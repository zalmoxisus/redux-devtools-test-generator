    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "codemirror": "^5.16.0",
    "es6template": "^1.0.4",
    "javascript-stringify": "^1.1.1",
